		function showAdAlert() {
			document.body.innerHTML += '<div style="display: flex; width: 100vw; height: 100vh; background-color: #1a1a1ab0; position: fixed; top: 0; right: 0; z-index: 99999; "><div style="width: 300px; height: fit-content; background-color: #000; margin: auto; border-radius: 10px; padding: 25px; "><h6 style="color: #dc3545; font-size: 21px; text-align: center; ">Adblock detectado!</h6><p style="color: #fff; margin-top: 16px; ">Por favor desabilite o adblock. Devido ao crescente número de usuários usando adblock tem se tornado cada vez mais difícil manter o site online. Sua colaboração faz toda a diferença para que possamos continuar aqui. Com seu apoio, podemos seguir crescendo e melhorando sempre. Agradecemos por fazer parte da nossa comunidade!</p></div></div>';
		}

		window.chromeadblocked = false;

		(async function() {
			const googleAdUrl = 'https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js';

			try {
				await fetch(new Request(googleAdUrl)).catch(_ => chromeadblocked = true)
			} catch (e) {
				chromeadblocked = true
			} finally {
				if (window.chromeadblocked === true) {
					showAdAlert();
				} else {
					setTimeout(function() {
						var adBoxEl = document.querySelector(".ad-box");
						var hasAdBlock = window.getComputedStyle(adBoxEl)?.display === "none";

						if (hasAdBlock) {
							showAdAlert();
						}
					}, 2000);
				}
			}
		})();
	</script>

